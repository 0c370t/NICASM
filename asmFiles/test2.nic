$VARR     .BLK  x05
$VAR      .FILL xFFFF

*START    ADD R0,R0,R1
          ADD R0,R0,#5
          ADD R0,R0,xF

          BR      *START
          BRN     *START
          BRZ     *START
          BRP     *START
          BRNZ    *START
          BRZP    *LABEL
          BRNP    *START
*LABEL    BR      *LABEL
          JMP     R7
          RET

          .SUB R0,R1,R1
          .MUL R0,R1,R1
