
$INTP				.FILL x0030
$TEN				.FILL xFFF6

					.MAIN
					LD R5, $TEN
					LD R3, $INTP
					ADD R2, R2, xA
					NOT R4, R3
					ADD R4, R4, #1


 					PRINT '>'
					READ R0
					ADD R0, R0, R4
					PRINT R2

					PRINT '>'
					READ R1
					ADD R1, R1, R4
					PRINT R2

					ADD R0, R0, R1

					PRINT '='
					READ R7
					ADD R5, R5, R0
					BRZ *SKIP
					PRINT '1'
					AND R0, R0, #0
*SKIP				ADD R0, R0, R3
					PRINT R0
